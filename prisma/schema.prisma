generator client {
  provider = "prisma-client-js"
  output = "../node_modules/.prisma/client"}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Member {
  id         Int  @default(autoincrement())  @id  @map("member_id")
  email      String
  createdAt  DateTime  @default(now()) @map("created_at")
}

model Event {

}

model Ticket {

}

enum TicketStatus {

}